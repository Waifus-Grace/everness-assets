        "DailyContent": [
          {
            "ID": "Daily01",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "meet_001",
              "SourceString": "初次见面",
              "LocalizedString": "初次见面"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_meet_001",
              "SourceString": "名字，弥斯墨。月光当铺只接待自愿交易的客人，但是只想许愿却不肯支付代价的话，还是做梦来得快点。",
              "LocalizedString": "名字，弥斯墨。月光当铺只接待自愿交易的客人，但是只想许愿却不肯支付代价的话，还是做梦来得快点。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_meet_001.Play_Vo_Char_NPC020_meet_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily02",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "meet_002",
              "SourceString": "再次见面",
              "LocalizedString": "再次见面"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_meet_002",
              "SourceString": "虽然名义上月光当铺已经消失了，但我不喜欢欠下人情。有需要的话就来找我吧，可以帮你一次。",
              "LocalizedString": "虽然名义上月光当铺已经消失了，但我不喜欢欠下人情。有需要的话就来找我吧，可以帮你一次。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_meet_002.Play_Vo_Char_NPC020_meet_002",
              "SubPathString": ""
            },
            "Conditions": [
              {
                "Firendly": 2,
                "QuestId": "None",
                "CharacterFileBreakLevel": 0
              }
            ]
          },
          {
            "ID": "Daily03",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC020_001",
              "SourceString": "闲聊·呢喃",
              "LocalizedString": "闲聊·呢喃"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_chat_001",
              "SourceString": "哥哥……没什么，你听错了。",
              "LocalizedString": "哥哥……没什么，你听错了。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_chat_001.Play_Vo_Char_NPC020_chat_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily04",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC020_002",
              "SourceString": "闲聊·鸟雀",
              "LocalizedString": "闲聊·鸟雀"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_chat_002",
              "SourceString": "叽叽喳喳，好吵。",
              "LocalizedString": "叽叽喳喳，好吵。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_chat_002.Play_Vo_Char_NPC020_chat_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily05",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC020_003",
              "SourceString": "闲聊·悠闲",
              "LocalizedString": "闲聊·悠闲"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_chat_003",
              "SourceString": "难得没有无趣的人打扰，泡壶好茶吧。",
              "LocalizedString": "难得没有无趣的人打扰，泡壶好茶吧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_chat_003.Play_Vo_Char_NPC020_chat_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily06",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_001",
              "SourceString": "大晴天",
              "LocalizedString": "大晴天"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_weather_001",
              "SourceString": "久违的阳光，出来走走倒也不错。",
              "LocalizedString": "久违的阳光，出来走走倒也不错。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_weather_001.Play_Vo_Char_NPC020_weather_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily07",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_002",
              "SourceString": "雨天",
              "LocalizedString": "雨天"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_weather_002",
              "SourceString": "麻烦的天气，早点回去吧。",
              "LocalizedString": "麻烦的天气，早点回去吧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_weather_002.Play_Vo_Char_NPC020_weather_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily08",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_003",
              "SourceString": "寒冷",
              "LocalizedString": "寒冷"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_weather_003",
              "SourceString": "无能的人才会选择依靠他人取暖。",
              "LocalizedString": "无能的人才会选择依靠他人取暖。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_weather_003.Play_Vo_Char_NPC020_weather_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily09",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_004",
              "SourceString": "炎热",
              "LocalizedString": "炎热"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_weather_004",
              "SourceString": "这种天气麻烦也会变少，这么一想，倒也不错。",
              "LocalizedString": "这种天气麻烦也会变少，这么一想，倒也不错。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_weather_004.Play_Vo_Char_NPC020_weather_004",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily10",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_001",
              "SourceString": "早上问候",
              "LocalizedString": "早上问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_greet_001",
              "SourceString": "休息好了就出发吧。",
              "LocalizedString": "休息好了就出发吧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_greet_001.Play_Vo_Char_NPC020_greet_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily11",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_002",
              "SourceString": "中午问候",
              "LocalizedString": "中午问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_greet_002",
              "SourceString": "想要我来做午饭？……算了，下不为例。",
              "LocalizedString": "想要我来做午饭？……算了，下不为例。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_greet_002.Play_Vo_Char_NPC020_greet_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily12",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_003",
              "SourceString": "傍晚问候",
              "LocalizedString": "傍晚问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_greet_003",
              "SourceString": "散步消食就免了，坐着看看月亮吧。",
              "LocalizedString": "散步消食就免了，坐着看看月亮吧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_greet_003.Play_Vo_Char_NPC020_greet_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily13",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_004",
              "SourceString": "晚安",
              "LocalizedString": "晚安"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_greet_004",
              "SourceString": "准备营业。",
              "LocalizedString": "准备营业。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_greet_004.Play_Vo_Char_NPC020_greet_004",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily14",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "hobby_001",
              "SourceString": "爱好",
              "LocalizedString": "爱好"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_hobby_001",
              "SourceString": "我没什么喜好……你说小动物？那是因为哥哥喜欢罢了。",
              "LocalizedString": "我没什么喜好……你说小动物？那是因为哥哥喜欢罢了。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_hobby_001.Play_Vo_Char_NPC020_hobby_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily15",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "vexation_001",
              "SourceString": "烦恼",
              "LocalizedString": "烦恼"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_vexation_001",
              "SourceString": "来月光当铺的，多的是自寻烦恼的人，我可不是。不过要是有人明明已经实现了愿望却不按照规定支付代价，那确实有些麻烦啊。",
              "LocalizedString": "来月光当铺的，多的是自寻烦恼的人，我可不是。不过要是有人明明已经实现了愿望却不按照规定支付代价，那确实有些麻烦啊。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_vexation_001.Play_Vo_Char_NPC020_vexation_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily16",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC020_001",
              "SourceString": "话题·异象",
              "LocalizedString": "话题·异象"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_talk_001",
              "SourceString": "当人的执念足够强烈时，也许能等到来自异象的回应……和异象做交易，又或者和人做交易，谈不上哪个更好，我能给你的忠告只有——得到的同时必然也会失去。",
              "LocalizedString": "当人的执念足够强烈时，也许能等到来自异象的回应……和异象做交易，又或者和人做交易，谈不上哪个更好，我能给你的忠告只有——得到的同时必然也会失去。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_talk_001.Play_Vo_Char_NPC020_talk_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily17",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC020_002",
              "SourceString": "话题·料理",
              "LocalizedString": "话题·料理"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_talk_002",
              "SourceString": "料理是很能静下心来的活动，不用和人打交道，也不需要别人来指手画脚，全凭自己的心意。想吃我做的料理？看我心情吧。",
              "LocalizedString": "料理是很能静下心来的活动，不用和人打交道，也不需要别人来指手画脚，全凭自己的心意。想吃我做的料理？看我心情吧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_talk_002.Play_Vo_Char_NPC020_talk_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily18",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC020_003",
              "SourceString": "话题·完美主义",
              "LocalizedString": "话题·完美主义"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_talk_003",
              "SourceString": "既然已经下定决心准备做了，就该做到最好。那些总是半途而废或草草了事的人，才更难以理解吧？",
              "LocalizedString": "既然已经下定决心准备做了，就该做到最好。那些总是半途而废或草草了事的人，才更难以理解吧？"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_talk_003.Play_Vo_Char_NPC020_talk_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily19",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_talk_004",
              "SourceString": "残疾的丈夫为了能和妻子共跳一支舞，典当了自己的「歌声」；而失聪的妻子为了能听到丈夫的歌声又当掉了自己的「舞步」……或许听起来是个「玩笑」，但比起那些无聊的愿望，还算是值得分享。",
              "LocalizedString": "残疾的丈夫为了能和妻子共跳一支舞，典当了自己的「歌声」；而失聪的妻子为了能听到丈夫的歌声又当掉了自己的「舞步」……或许听起来是个「玩笑」，但比起那些无聊的愿望，还算是值得分享。"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC020_004",
              "SourceString": "话题·愿望",
              "LocalizedString": "话题·愿望"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_talk_004.Play_Vo_Char_NPC020_talk_004",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily20",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "self_NPC020_001",
              "SourceString": "关于自己·异能",
              "LocalizedString": "关于自己·异能"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_self_001",
              "SourceString": "等价交换是交易的前提，他们实现愿望，而我收取代价，这是很合理的行为。看在你帮过我的份上，提醒你一句，商人可不会做亏本生意。",
              "LocalizedString": "等价交换是交易的前提，他们实现愿望，而我收取代价，这是很合理的行为。看在你帮过我的份上，提醒你一句，商人可不会做亏本生意。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_self_001.Play_Vo_Char_NPC020_self_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily21",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "self_NPC020_002",
              "SourceString": "关于自己·吊坠",
              "LocalizedString": "关于自己·吊坠"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_self_002",
              "SourceString": "双生子的诅咒？无稽之谈。相信这种话的人，不过是命运的奴隶。就算这种无聊的假设真的发生了，消失的那个人也不该是哥哥。",
              "LocalizedString": "双生子的诅咒？无稽之谈。相信这种话的人，不过是命运的奴隶。就算这种无聊的假设真的发生了，消失的那个人也不该是哥哥。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_self_002.Play_Vo_Char_NPC020_self_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily22",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "self_NPC020_003",
              "SourceString": "关于自己·寡言",
              "LocalizedString": "关于自己·寡言"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_self_003",
              "SourceString": "没有必要而已，聒噪的人太多了。",
              "LocalizedString": "没有必要而已，聒噪的人太多了。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC020/Play_Vo_Char_NPC020_self_003.Play_Vo_Char_NPC020_self_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily23",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "about_NPC019_001",
              "SourceString": "关于奥托·幼时",
              "LocalizedString": "关于奥托·幼时"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC020_about_001",
              "SourceString": "小时候哥哥总是挡在我前面，甚至那个时候……哈？他说我小时候比现在坦率多了？他的话你也信？",
              "LocalizedString": "小时候哥哥总是挡在我前面，甚至那个时候……哈？他说我小时候比现在坦率多了？他的话你也信？"
            },
            "AudioEvent": {
