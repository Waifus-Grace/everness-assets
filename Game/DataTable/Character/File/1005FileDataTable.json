        "DailyContent": [
          {
            "ID": "Daily01",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "meet_001",
              "SourceString": "初次见面",
              "LocalizedString": "初次见面"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_meet_001",
              "SourceString": "你好，我是达芙蒂尔，浔的保镖……太简单了吗？抱歉，我并不擅长闲聊，有需要请直接咨询「伊波恩」古董店。",
              "LocalizedString": "你好，我是达芙蒂尔，浔的保镖……太简单了吗？抱歉，我并不擅长闲聊，有需要请直接咨询「伊波恩」古董店。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_meet_001.Play_Vo_Char_NPC013_meet_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily02",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC013_001",
              "SourceString": "闲聊·休息",
              "LocalizedString": "闲聊·休息"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_chat_001",
              "SourceString": "找点事情做做吧，总是闲着的话身体会生锈的。",
              "LocalizedString": "找点事情做做吧，总是闲着的话身体会生锈的。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_chat_001.Play_Vo_Char_NPC013_chat_001",
              "SubPathString": ""
            },
            "Conditions": [
              {
                "Firendly": 2,
                "QuestId": "None",
                "CharacterFileBreakLevel": 0
              }
            ]
          },
          {
            "ID": "Daily03",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC013_002",
              "SourceString": "闲聊·武器",
              "LocalizedString": "闲聊·武器"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_chat_002",
              "SourceString": "再好的武器也需要定期保养才能保持锋利，人也是一样。",
              "LocalizedString": "再好的武器也需要定期保养才能保持锋利，人也是一样。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_chat_002.Play_Vo_Char_NPC013_chat_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily04",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "chat_NPC013_003",
              "SourceString": "闲聊·欢迎词",
              "LocalizedString": "闲聊·欢迎词"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_chat_003",
              "SourceString": "亲、亲爱的顾客，欢迎光……光……啧。",
              "LocalizedString": "亲、亲爱的顾客，欢迎光……光……啧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_chat_003.Play_Vo_Char_NPC013_chat_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily05",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_001",
              "SourceString": "大晴天",
              "LocalizedString": "大晴天"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_weather_001",
              "SourceString": "好天气，适合户外训练。",
              "LocalizedString": "好天气，适合户外训练。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_weather_001.Play_Vo_Char_NPC013_weather_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily06",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_002",
              "SourceString": "雨天",
              "LocalizedString": "雨天"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_weather_002",
              "SourceString": "下雨了？利用雨水隐蔽身形和气味的话……啧，下意识又……可恶。",
              "LocalizedString": "下雨了？利用雨水隐蔽身形和气味的话……啧，下意识又……可恶。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_weather_002.Play_Vo_Char_NPC013_weather_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily07",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_003",
              "SourceString": "寒冷",
              "LocalizedString": "寒冷"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_weather_003",
              "SourceString": "寒冷会让身体僵硬，麻痹感知……或许这时候，猎物和猎手的身份正适合互换。",
              "LocalizedString": "寒冷会让身体僵硬，麻痹感知……或许这时候，猎物和猎手的身份正适合互换。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_weather_003.Play_Vo_Char_NPC013_weather_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily08",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "weather_004",
              "SourceString": "炎热",
              "LocalizedString": "炎热"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_weather_004",
              "SourceString": "呼……令人讨厌的温度。",
              "LocalizedString": "呼……令人讨厌的温度。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_weather_004.Play_Vo_Char_NPC013_weather_004",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily09",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_001",
              "SourceString": "早上问候",
              "LocalizedString": "早上问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_greet_001",
              "SourceString": "早上好，今天的安排是什么？",
              "LocalizedString": "早上好，今天的安排是什么？"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_greet_001.Play_Vo_Char_NPC013_greet_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily10",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_002",
              "SourceString": "中午问候",
              "LocalizedString": "中午问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_greet_002",
              "SourceString": "困的话可以小憩一会儿，我会叫醒你的。",
              "LocalizedString": "困的话可以小憩一会儿，我会叫醒你的。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_greet_002.Play_Vo_Char_NPC013_greet_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily11",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_003",
              "SourceString": "傍晚问候",
              "LocalizedString": "傍晚问候"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_greet_003",
              "SourceString": "还能再训练一会儿，要来么？",
              "LocalizedString": "还能再训练一会儿，要来么？"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_greet_003.Play_Vo_Char_NPC013_greet_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily12",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "greet_004",
              "SourceString": "晚安",
              "LocalizedString": "晚安"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_greet_004",
              "SourceString": "好好休息吧，夜里的伊波恩，很安全。",
              "LocalizedString": "好好休息吧，夜里的伊波恩，很安全。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_greet_004.Play_Vo_Char_NPC013_greet_004",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily13",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "hobby_001",
              "SourceString": "爱好",
              "LocalizedString": "爱好"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_hobby_001",
              "SourceString": "谈不上有什么特别喜好，生存需要而已。压缩甜食便于携带又可以提供大量能量，所以我会带上一些以备不时之需。至于口味，那并不重要。",
              "LocalizedString": "谈不上有什么特别喜好，生存需要而已。压缩甜食便于携带又可以提供大量能量，所以我会带上一些以备不时之需。至于口味，那并不重要。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_hobby_001.Play_Vo_Char_NPC013_hobby_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily14",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "vexation_001",
              "SourceString": "烦恼",
              "LocalizedString": "烦恼"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_vexation_001",
              "SourceString": "有些客人比异象还要难缠，但是浔说为了伊波恩的生意，一定要保持微笑服务……感觉最近的委托成交率更低了？咳……是你的错觉。",
              "LocalizedString": "有些客人比异象还要难缠，但是浔说为了伊波恩的生意，一定要保持微笑服务……感觉最近的委托成交率更低了？咳……是你的错觉。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_vexation_001.Play_Vo_Char_NPC013_vexation_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily15",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC013_001",
              "SourceString": "话题·异象",
              "LocalizedString": "话题·异象"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_talk_001",
              "SourceString": "有一种异象会夺走人的记忆，失去了名字的人是逃不出去的，除非你自愿放弃被夺走的一切，换取空茫可悲的自由……要是遇到了，就跑快点儿。为什么会这么清楚？……有过一些渊源罢了。",
              "LocalizedString": "有一种异象会夺走人的记忆，失去了名字的人是逃不出去的，除非你自愿放弃被夺走的一切，换取空茫可悲的自由……要是遇到了，就跑快点儿。为什么会这么清楚？……有过一些渊源罢了。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_talk_001.Play_Vo_Char_NPC013_talk_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily16",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC013_002",
              "SourceString": "话题·训练",
              "LocalizedString": "话题·训练"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_talk_002",
              "SourceString": "日复一日的训练算不上枯燥，想要变得更强，总得付出一些代价。只有足够强大，才不会重蹈覆辙。",
              "LocalizedString": "日复一日的训练算不上枯燥，想要变得更强，总得付出一些代价。只有足够强大，才不会重蹈覆辙。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_talk_002.Play_Vo_Char_NPC013_talk_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily17",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "talk_NPC013_003",
              "SourceString": "话题·钓鱼",
              "LocalizedString": "话题·钓鱼"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_talk_003",
              "SourceString": "我观察过那些坐在岸边一整天却毫无收获的钓者，天气，水温，鱼群状态，太多不可控制的因素都会导致失败，但耐心始终是一个好的猎手该具备的品质，鱼桶总会有装满的时候。",
              "LocalizedString": "我观察过那些坐在岸边一整天却毫无收获的钓者，天气，水温，鱼群状态，太多不可控制的因素都会导致失败，但耐心始终是一个好的猎手该具备的品质，鱼桶总会有装满的时候。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_talk_003.Play_Vo_Char_NPC013_talk_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily18",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "self_NPC013_001",
              "SourceString": "关于自己·异能",
              "LocalizedString": "关于自己·异能"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_self_001",
              "SourceString": "只是用一小部分视力作为交换而已，并不影响日常生活，正相反，有时听见的可比看见的更多。",
              "LocalizedString": "只是用一小部分视力作为交换而已，并不影响日常生活，正相反，有时听见的可比看见的更多。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_self_001.Play_Vo_Char_NPC013_self_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily19",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "self_NPC013_002",
              "SourceString": "关于自己·深色",
              "LocalizedString": "关于自己·深色"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_self_002",
              "SourceString": "总是穿深色的衣服？……并不是什么偏好，只是深色的衣服能掩盖受伤的痕迹，多数时候可以用来欺骗对手。",
              "LocalizedString": "总是穿深色的衣服？……并不是什么偏好，只是深色的衣服能掩盖受伤的痕迹，多数时候可以用来欺骗对手。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_self_002.Play_Vo_Char_NPC013_self_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily20",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "self_NPC013_003",
              "SourceString": "关于自己·名字",
              "LocalizedString": "关于自己·名字"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_self_003",
              "SourceString": "是水仙花的意思。浔说水仙是一种孤高自洁的花，我不觉得跟我有什么相似之处，花都太过娇弱了，不过听说它很好养活。",
              "LocalizedString": "是水仙花的意思。浔说水仙是一种孤高自洁的花，我不觉得跟我有什么相似之处，花都太过娇弱了，不过听说它很好养活。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_self_003.Play_Vo_Char_NPC013_self_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily21",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "about_NPC009_001",
              "SourceString": "关于浔·担忧",
              "LocalizedString": "关于浔·担忧"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_about_001",
              "SourceString": "有时候浔的做法过于大胆了，总是以身涉险，就好像只要能达成想要的结果，什么都无所谓，呼……是我还不够强。",
              "LocalizedString": "有时候浔的做法过于大胆了，总是以身涉险，就好像只要能达成想要的结果，什么都无所谓，呼……是我还不够强。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_about_001.Play_Vo_Char_NPC013_about_001",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily22",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "about_NPC009_002",
              "SourceString": "关于浔·小水仙",
              "LocalizedString": "关于浔·小水仙"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_about_002",
              "SourceString": "咳咳……浔有时候喜欢开玩笑。虽然我并不介意这个称呼，但还请不要这么叫我。",
              "LocalizedString": "咳咳……浔有时候喜欢开玩笑。虽然我并不介意这个称呼，但还请不要这么叫我。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_about_002.Play_Vo_Char_NPC013_about_002",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily23",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "about_NPC011",
              "SourceString": "关于娜娜莉",
              "LocalizedString": "关于娜娜莉"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_about_003",
              "SourceString": "那孩子总是在我训练的时候偷偷观察，就算被发现了过一会儿又会藏到另一个地方继续看……算了，只要不妨碍我，就随她吧。",
              "LocalizedString": "那孩子总是在我训练的时候偷偷观察，就算被发现了过一会儿又会藏到另一个地方继续看……算了，只要不妨碍我，就随她吧。"
            },
            "AudioEvent": {
              "AssetPathName": "/Game/WwiseAudio/Events/Voice/Char/NPC013/Play_Vo_Char_NPC013_about_003.Play_Vo_Char_NPC013_about_003",
              "SubPathString": ""
            },
            "Conditions": []
          },
          {
            "ID": "Daily24",
            "Title": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "about_NPC022",
              "SourceString": "关于阿多尼丝",
              "LocalizedString": "关于阿多尼丝"
            },
            "Content": {
              "TableId": "/Game/Text/ST_File.ST_File",
              "Key": "NPC013_about_004",
              "SourceString": "很危险，如非必要，不要和她有任何接触。",
              "LocalizedString": "很危险，如非必要，不要和她有任何接触。"
